<xs:schema xmlns:elvis="http://www.iscev.org/ElVisML-v1" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:dsig="http://www.w3.org/2000/09/xmldsig#" xmlns:enc="http://www.w3.org/2001/04/xmlenc#" targetNamespace="http://www.iscev.org/ElVisML-v1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0" xml:lang="en">
	<xs:import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="http://www.w3.org/TR/xmldsig-core/xmldsig-core-schema.xsd"/>
	<xs:import namespace="http://www.w3.org/2001/04/xmlenc#" schemaLocation="http://www.w3.org/TR/xmlenc-core/xenc-schema.xsd"/>
	<xs:element name="ElVis">
		<xs:annotation>
			<xs:documentation>Root element</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:all>
				<xs:element name="Subjects" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Contains a list of one or mor subjects (e. g. patients). May be omited if only protocols should be transfered. Obligatory if one or more tests are transfered.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence minOccurs="1">
							<xs:element name="Subject" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Contains informations to a subject (e. g. patient) of a test. All child elements are optional. The id attribute must be set.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:choice>
										<xs:sequence>
											<xs:element name="Lastname" type="xs:string" minOccurs="0">
												<xs:annotation>
													<xs:documentation>The last name of a subject</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="Firstname" type="xs:string" minOccurs="0">
												<xs:annotation>
													<xs:documentation>The first name of a subject.</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="Birthdate" type="xs:date" minOccurs="0">
												<xs:annotation>
													<xs:documentation>The date of birth of a subject</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="Gender" minOccurs="0">
												<xs:simpleType>
													<xs:restriction base="xs:token">
														<xs:enumeration value="male"/>
														<xs:enumeration value="female"/>
														<xs:enumeration value="unspecified"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="EyeColor" type="xs:token" minOccurs="0"/>
											<xs:element name="EthnicOrigin" type="xs:token" minOccurs="0"/>
											<xs:element name="ContactInformation" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Contains informations of the address of a subject. All child elements are optional.</xs:documentation>
												</xs:annotation>
												<xs:complexType>
													<xs:sequence>
														<xs:element name="Street" type="xs:string" minOccurs="0">
															<xs:annotation>
																<xs:documentation>The street of the address of a subject</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="ZipCode" type="xs:string" minOccurs="0">
															<xs:annotation>
																<xs:documentation>The zip code (postal code) of the address of a subject</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="City" type="xs:string" minOccurs="0">
															<xs:annotation>
																<xs:documentation>The city of the address of a subject</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="Country" type="xs:string" minOccurs="0">
															<xs:annotation>
																<xs:documentation>The country of the address of a subject</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="Phone" type="xs:string" minOccurs="0">
															<xs:annotation>
																<xs:documentation>The phone number of a subject</xs:documentation>
															</xs:annotation>
														</xs:element>
														<xs:element name="EMail" type="xs:string" minOccurs="0"/>
													</xs:sequence>
												</xs:complexType>
											</xs:element>
											<xs:element name="Notes" type="xs:string" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Additional comments to a subject</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element ref="elvis:CustomAttributes" minOccurs="0">
												<xs:annotation>
													<xs:documentation>May be used to store additional informations to the subject as a list of key value pairs.</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
										<xs:element ref="enc:EncryptedData"/>
									</xs:choice>
									<xs:attribute name="id" type="xs:NCName" use="required">
										<xs:annotation>
											<xs:documentation>Id which must be unique inside the document. Used to link a RecordingSession with a subject</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="normal" type="xs:boolean" use="optional" default="false">
										<xs:annotation>
											<xs:documentation>Used to indicate wether this subject is a healthy or a control subject</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="externalIdentifier" type="xs:string">
										<xs:annotation>
											<xs:documentation>Used for external IDs e. g. patient ID in a HIS</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Protocols" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Contains a list of one or more protocols. May be omited if only subject information is transfered. Obligatory if one or more tests is transfered.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence minOccurs="1">
							<xs:element name="Protocol" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>A set of stimuli and recording settings used in a test</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Name" type="xs:string">
											<xs:annotation>
												<xs:documentation>The name of the protocol</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Type" type="xs:string" minOccurs="0"/>
										<xs:element name="Description" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Description of the protocol</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Notes" type="xs:string" minOccurs="0"/>
										<xs:element name="StimulusDevice" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Used as a hint which device should be used for stimulation, e. g. Ganzfeld, Mini-Ganzfeld, ...</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="Electrodes" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Electrode" minOccurs="2" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Name" type="xs:token"/>
																<xs:element name="Type" minOccurs="0">
																	<xs:simpleType>
																		<xs:restriction base="xs:token">
																			<xs:enumeration value="Gold Lens"/>
																			<xs:enumeration value="Burian-Allen"/>
																			<xs:enumeration value="DTL"/>
																			<xs:enumeration value="Gold Leaf"/>
																			<xs:enumeration value="Jet"/>
																			<xs:enumeration value="Skin"/>
																			<xs:enumeration value="Cup"/>
																			<xs:enumeration value="Other"/>
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="Position" type="xs:string" minOccurs="0"/>
																<xs:element ref="elvis:CustomAttributes" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>additional information for the electrode</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
															<xs:attribute name="id" type="xs:NCName" use="required"/>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Leads" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Lead" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Name" type="xs:token" minOccurs="0"/>
																<xs:element name="Electrodes" minOccurs="0">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="Electrode" maxOccurs="unbounded">
																				<xs:complexType>
																					<xs:attribute name="ref" type="xs:NCName" use="required"/>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
															<xs:attribute name="id" type="xs:NCName" use="required"/>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="SequenceDefinition" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Contains a list of one or more steps the protocol consists of</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence minOccurs="0">
													<xs:element name="StepDefinition" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Contains settings for a specific step like stimulus and acquisition parameters</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Name" type="xs:string">
																	<xs:annotation>
																		<xs:documentation>Name of the step</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="AdaptationTime" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Time to wait before starting to record results</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:simpleContent>
																			<xs:extension base="elvis:MeasurementType">
																				<xs:attribute name="type">
																					<xs:simpleType>
																						<xs:restriction base="xs:token">
																							<xs:enumeration value="dark"/>
																							<xs:enumeration value="light"/>
																						</xs:restriction>
																					</xs:simpleType>
																				</xs:attribute>
																			</xs:extension>
																		</xs:simpleContent>
																	</xs:complexType>
																</xs:element>
																<xs:element name="AcquisitionSettings" minOccurs="1">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="PreTriggerTime" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Duration to record signals before trigger (stimuls). Used for e. g. auto-zero</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="elvis:MeasurementType"/>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="PostTriggerTime" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Duration to record signals after the trigger</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="elvis:MeasurementType"/>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="SamplingRate" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Defines the number of samples per second.</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="elvis:MeasurementType"/>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="NumberOfAverages" type="xs:positiveInteger" default="1" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Number of results that should be recorded</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="InterAverageDelay" type="elvis:MeasurementType" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Time between two averages</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="SamplesPerAverage" type="xs:positiveInteger" default="1" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Mininum number of sweeps per average</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="InterSampleDelay" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Time between two sweeps</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="elvis:MeasurementType"/>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="Stimulus" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Settings of the stimulus used for a step</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:choice>
																			<xs:element name="ERG" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Electroretinogram</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:choice>
																						<xs:element name="Flicker" minOccurs="0">
																							<xs:complexType>
																								<xs:sequence>
																									<xs:element name="FlashFrequency" minOccurs="0">
																										<xs:annotation>
																											<xs:documentation>If continous stimulation (flicker ERG) is used, defines the frequency of the flicker</xs:documentation>
																										</xs:annotation>
																										<xs:complexType>
																											<xs:simpleContent>
																												<xs:extension base="elvis:MeasurementType"/>
																											</xs:simpleContent>
																										</xs:complexType>
																									</xs:element>
																									<xs:group ref="elvis:stimulusGroup"/>
																									<xs:element ref="elvis:CustomAttributes" minOccurs="0">
																										<xs:annotation>
																											<xs:documentation>Additional informations about the stimulus</xs:documentation>
																										</xs:annotation>
																									</xs:element>
																								</xs:sequence>
																							</xs:complexType>
																						</xs:element>
																						<xs:element name="Flash" minOccurs="0" maxOccurs="unbounded">
																							<xs:complexType>
																								<xs:sequence>
																									<xs:element name="InterStimulusInterval" type="elvis:MeasurementType" minOccurs="0">
																										<xs:annotation>
																											<xs:documentation>Time before flash</xs:documentation>
																										</xs:annotation>
																									</xs:element>
																									<xs:group ref="elvis:stimulusGroup"/>
																									<xs:element ref="elvis:CustomAttributes" minOccurs="0">
																										<xs:annotation>
																											<xs:documentation>Additional informations about the stimulus</xs:documentation>
																										</xs:annotation>
																									</xs:element>
																								</xs:sequence>
																							</xs:complexType>
																						</xs:element>
																					</xs:choice>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="MultifocalERG" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Multifocal Electroretinogram</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="StimulusSize" type="elvis:MeasurementType">
																							<xs:annotation>
																								<xs:documentation>Size of the stimulus in degrees</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="NumberOfHexagons" type="xs:int">
																							<xs:annotation>
																								<xs:documentation>The number of the hexagons</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="Contrast" type="xs:double">
																							<xs:annotation>
																								<xs:documentation>The contrast</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="StimulusStrength">
																							<xs:annotation>
																								<xs:documentation>The stimulus intensity of black and white hexagons</xs:documentation>
																							</xs:annotation>
																							<xs:complexType>
																								<xs:sequence>
																									<xs:element name="Black" type="elvis:MeasurementType"/>
																									<xs:element name="White" type="elvis:MeasurementType"/>
																								</xs:sequence>
																							</xs:complexType>
																						</xs:element>
																						<xs:element name="BackgroundIntensity">
																							<xs:annotation>
																								<xs:documentation>The background intensity</xs:documentation>
																							</xs:annotation>
																							<xs:complexType>
																								<xs:simpleContent>
																									<xs:extension base="elvis:MeasurementType"/>
																								</xs:simpleContent>
																							</xs:complexType>
																						</xs:element>
																						<xs:element name="MSequenceIterations" type="xs:int">
																							<xs:annotation>
																								<xs:documentation>The number of iterations of the m-Sequence</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="Fixation" type="xs:boolean"/>
																						<xs:element ref="elvis:CustomAttributes">
																							<xs:annotation>
																								<xs:documentation>Additional informations about the stimulus</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="EOG" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Electrooculogram</xs:documentation>
																				</xs:annotation>
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="Angle" type="elvis:MeasurementArrayType" minOccurs="0"/>
																						<xs:element name="ReversalFrequency" type="elvis:MeasurementType"/>
																						<xs:element name="StimulusStrength" type="elvis:MeasurementType"/>
																						<xs:element name="BackgroundLuminance">
																							<xs:annotation>
																								<xs:documentation>The background intensity</xs:documentation>
																							</xs:annotation>
																							<xs:complexType>
																								<xs:simpleContent>
																									<xs:extension base="elvis:MeasurementType"/>
																								</xs:simpleContent>
																							</xs:complexType>
																						</xs:element>
																						<xs:element name="BackgroundColor" type="xs:token">
																							<xs:annotation>
																								<xs:documentation>The color of the background ilumination</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="AudibleIndicator" type="xs:boolean"/>
																						<xs:element ref="elvis:CustomAttributes" minOccurs="0">
																							<xs:annotation>
																								<xs:documentation>Additional informations about the stimulus</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="PatternERG" minOccurs="0">
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="Refraction" minOccurs="0">
																							<xs:annotation>
																								<xs:documentation>????</xs:documentation>
																							</xs:annotation>
																							<xs:complexType>
																								<xs:sequence>
																									<xs:element name="OD" type="elvis:MeasurementType"/>
																									<xs:element name="OS" type="elvis:MeasurementType"/>
																								</xs:sequence>
																							</xs:complexType>
																						</xs:element>
																						<xs:element name="Distance" type="elvis:MeasurementType">
																							<xs:annotation>
																								<xs:documentation>The distance to the stimulus</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="PatternType" type="xs:token">
																							<xs:annotation>
																								<xs:documentation>The type of pattern used for stimulation (e. g. checker board)</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="SpatialFrequency" type="elvis:MeasurementType">
																							<xs:annotation>
																								<xs:documentation>The spatial frequency of the stimulus</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="FieldSize" type="elvis:MeasurementType">
																							<xs:annotation>
																								<xs:documentation>The field size of the stimulus</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="Contrast" type="xs:double">
																							<xs:annotation>
																								<xs:documentation>The contrast of the stimulus</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="Intensity" type="elvis:MeasurementType">
																							<xs:annotation>
																								<xs:documentation>The intensity of the stimulus</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element name="AmbientLumiance" type="elvis:MeasurementType" minOccurs="0">
																							<xs:annotation>
																								<xs:documentation>The intensity of the ambient luminance</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																						<xs:element ref="elvis:CustomAttributes" minOccurs="0">
																							<xs:annotation>
																								<xs:documentation>Additional informations about the stimulus</xs:documentation>
																							</xs:annotation>
																						</xs:element>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="VEP" type="xs:anyType" minOccurs="0">
																				<xs:annotation>
																					<xs:documentation>Visual evoked potentials</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="Extern" type="xs:anyType" minOccurs="0"/>
																			<xs:element name="Other" type="xs:anyType" minOccurs="0"/>
																		</xs:choice>
																	</xs:complexType>
																</xs:element>
																<xs:element name="ChannelDefinitions" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>Contains a list of one or more channels used for recording</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
